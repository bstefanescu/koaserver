{
  "name": "@koa-stack/server",
  "version": "0.6.3",
  "description": "A web server built on top of Koa, featuring advanced routing, a lazy body reader and customizable error handling.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rimraf ./lib && tsc --project ./tsconfig.build.json",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/all-tests.ts'"
  },
  "keywords": [
    "koa",
    "web",
    "server",
    "router",
    "routing",
    "route",
    "decorators",
    "jax-rs",
    "serve",
    "send",
    "static",
    "files",
    "lazy",
    "body",
    "custom",
    "error",
    "handling"
  ],
  "author": "Bogdan Stefanescu <stefanescu.bogdan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "formidable": "^2.0.1",
    "inflation": "^2.0.0",
    "koa": "^2.13.4",
    "koa-send": "^5.0.1",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.2",
    "raw-body": "^2.4.2",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/formidable": "^2.0.0",
    "@types/inflation": "^2.0.1",
    "@types/koa": "^2.13.4",
    "@types/koa-send": "^4.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.0",
    "@types/qs": "^6.9.7",
    "@types/statuses": "^2.0.0",
    "@types/supertest": "^2.0.11",
    "codecov": "^3.8.3",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0"
  }
